hi bhau

bhau baherun ghe printf bola;

bhau lakshat thev capital_vowels(){
    bhau hai ahe ch = 'A';
    bhau joparent(ch <= 'Z'){
        bhau jar(ch == 'A' || ch == 'E' || ch == 'I' || ch == 'O' || ch == 'U'){
            bola("%c ",ch);
        }bhau tar{
            bola("%c ",ch + 32);
        }
        ++ch;
    }
    bola("\n");
}

bhau lakshat thev print_characters(){
    capital_vowels();
    bola("\n");
}

bhau ithun chalu{
    print_characters();
}

bye bhau