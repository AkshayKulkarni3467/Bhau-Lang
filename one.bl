bhau lakshat thev float_control_flow(){
    bhau hai ahe y = 3.14;
    bhau hai ahe x = 2.71;
    bhau hai ahe z = 0.0;
    bhau jar(x > y){
        z = x - y;
    }bhau tar{
        z = x + y;
    }
    bhau jar(z > 5.0){
        bhau hai ahe double_z = z*2.0;
        printf("From float_control_flow, Value : %.2f\n",double_z);
    }
}



