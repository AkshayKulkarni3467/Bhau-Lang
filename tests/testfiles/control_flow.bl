bhau baherun ghe printf bolo;

bhau lakshat thev deep_nesting(){
    bhau hai ahe i = 1;
    bhau joparent(i <= 3){
        bhau jar(i % 2 == 0){
            bhau hai ahe j = i;
            bhau joparent(j > 0){
                bhau chunav(j){
                    bhau nivad 1:
                        bolo("From deep_nesting, choice = 1, when i = %d, j = %d\n",i,j);
                        bhau thamb;
                    bhau nivad 2:
                        bolo("From deep_nesting, choice = 2, when i = %d,j = %d\n",i,j);
                        bhau thamb;
                    bhau rahudet:
                        bolo("From deep_nesting, choice = default, when i = %d\n",i);
                        bhau thamb;
                }
                --j;
            }
        }bhau tar{
                bolo("From deep_nesting, else block when i = %d\n",i);
            }
        ++i;
    }
}

bhau lakshat thev while_if_while(){
    bhau hai ahe x = 1;
    bhau joparent(x <= 2){
        bhau jar(x == 1){
            bhau hai ahe i = 0;
            bhau joparent(i < 3){
                bhau chunav(i){
                    bhau nivad 0:
                        bolo("From while_if_while, i Value : %d\n",i);
                        bhau thamb;
                    
                    bhau nivad 1:
                        bolo("From while_if_while, i Value : %d\n",i);
                        bhau thamb;

                    bhau nivad 2:
                        bolo("From while_if_while, i Value : %d\n",i);
                        bhau thamb;
                }
                ++i;
            }
        }
        ++x;
    }
}

bhau lakshat thev if_in_switch(){
    bhau hai ahe i = 0;
    bhau joparent(i < 3){
        bhau hai ahe val = i+1;
        bhau chunav(val){
            bhau nivad 1:
                bhau jar(val%2 == 1){
                    bolo("From if_in_switch, i Value : %d\n",i);
                }
                bhau thamb;

            bhau nivad 2:
                bhau joparent(val > 0){
                    bolo("From if_in_switch, i Value : %d\n",i);
                    val-=1;
                }
                bhau thamb;
            
            bhau nivad 3:
                bolo("From if_in_switch, i Value : %d\n",i);
                bhau thamb;
        }
        ++i;
    }
}

bhau lakshat thev switch_in_if(){
    bhau hai ahe a = 3;
    bhau jar(a > 0){
        bhau joparent(a > 0){
            bhau chunav(a){
                bhau nivad 1: bolo("From switch_in_if, a Value : %d\n",a); bhau thamb;
                bhau nivad 2: bolo("From switch_in_if, a Value : %d\n",a); bhau thamb;
                bhau nivad 3: bolo("From switch_in_if, a Value : %d\n",a); bhau thamb;
            }
            --a;
        }
    }bhau tar {
        bolo("From switch_in_if, a Value : %d\n",a);
    }
}

bhau lakshat thev reuse_loop_counters(){
    bhau hai ahe i = 1;
    bhau joparent(i <= 2){
        bhau hai ahe k = i*2;
        bhau joparent(k > 0){
            bhau jar(k % 2 == 0){
                bolo("From reuse_loop_counters, k Value : %d\n",k);
            }bhau tar{
                bolo("From reuse_loop_counters, k Value : %d\n",k*10);
            }
            --k;
        }
        ++i;
    }
}

bhau lakshat thev recursively_growing_blocks(){
    bhau hai ahe i = 0;
    bhau joparent(i < 3){
        bhau jar(i == 1){
            bhau hai ahe j = 0;
            bhau joparent(j <= 1){
                bhau chunav(j){
                    bhau nivad 0: bolo("From growing_blocks, i Value : %d\n",i); bhau thamb;
                    bhau nivad 1: bolo("From growing_blocks, i Value : %d\n",i); bhau thamb;
                }
                ++j;
            }
        }bhau tar{
            bolo("From growing_blocks, i Value : %d\n",i);
        }
        ++i;
    }
}

bhau lakshat thev mixed_control_flow(){
    bhau hai ahe x = 1;
    bhau joparent(x <= 3){
        bhau jar(x == 2){
            bhau hai ahe y = 3;
            bhau joparent(y > 0){
                bhau chunav(y){
                    bhau nivad 1: bolo("From mixed_control_flow, y Value : %d\n",y); bhau thamb;
                    bhau nivad 2: bolo("From mixed_control_flow, y Value : %d\n",y); bhau thamb;
                    bhau nivad 3: bolo("From mixed_control_flow, y Value : %d\n",y); bhau thamb;
                }
                --y;
            }
        }bhau nahitar(x == 1){
            bolo("From mixed_control_flow, x Value : %d\n",x);
        }bhau tar{
            bolo("From mixed_control_flow, x Value : %d\n",x);
        }
        ++x;
    }
}

bhau lakshat thev while_if_switch(){
    bhau hai ahe i = 0;
    bhau joparent(i < 4){
        bhau jar(i % 2 == 0){
            bhau chunav(i){
                bhau nivad 0: bolo("From while_if_switch <if>, i Value : %d\n",i); bhau thamb;
                bhau nivad 2: bolo("From while_if_switch <if>, i Value : %d\n",i); bhau thamb;
            }
        }bhau tar{
            bolo("From while_if_switch <else>, i Value : %d\n",i);
        }
        ++i;
    }
}

bhau lakshat thev while_switch_if(){
    bhau hai ahe i = 1;
    bhau joparent(i <= 3){
        bhau chunav(i){
            bhau nivad 1:
                bhau jar(i % 2 == 1){
                    bolo("From while_switch_if, i Value : %d\n",i);
                }
                bhau thamb;
            bhau nivad 2:
                bhau jar(i * 2 == 4){
                    bolo("From while_switch_if, i Value : %d\n",i);
                }
                bhau thamb;
            bhau nivad 3:
                bhau jar(i - 1 > 0){
                    bolo("From while_switch_if, i Value : %d\n",i);
                }
                bhau thamb;
        }
        ++i;
    }
}

bhau lakshat thev while_if_while_switch(){
    bhau hai ahe i = 1;
    bhau joparent(i <= 3){
        bhau jar(i % 2 == 0){
            bolo("From while_if_while_switch <if>, i Value : %d\n",i);
        }bhau tar{
            bhau hai ahe j = 0;
            bhau joparent(j < 1){
                bhau chunav(i){
                    bhau nivad 1: bolo("From while_if_while_switch <else>, i Value : %d\n",i); bhau thamb;
                    bhau nivad 3: bolo("From while_if_while_switch <else>, i Value : %d\n",i); bhau thamb;
                }
                ++j;
            }
        }
        ++i;
    }
}

bhau lakshat thev print_control_flow(){
    deep_nesting();
    bolo("\n");
    while_if_while();
    bolo("\n");
    if_in_switch();
    bolo("\n");
    switch_in_if();
    bolo("\n");
    reuse_loop_counters();
    bolo("\n");
    recursively_growing_blocks();
    bolo("\n");
    mixed_control_flow();
    bolo("\n");
    while_if_switch();
    bolo("\n");
    while_switch_if();
    bolo("\n");
    while_if_while_switch();
    bolo("\n");
}

bhau ithun chalu{
    print_control_flow();
}


