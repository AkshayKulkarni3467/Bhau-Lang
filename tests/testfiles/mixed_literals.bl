    bhau jar((f + i > 6.299) && (c != 'a') && b && s1 == s2){
        oraoraoroara("From mixed_control_flow , value : %d\n",1);
    }bhau tar{
        oraoraoroara("From mixed_control_flow , value : %d\n",0);
    }
}

